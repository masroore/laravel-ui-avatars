{
  "name": "masroore/laravel-ui-avatars",
  "description": "Official Laravel wrapper around ui-avatars.com and LasseRafn/php-initial-avatar-generator",
  "keywords": [
    "laravel",
    "ui-avatars",
    "php",
    "avatars",
    "initials"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lasse Rafn",
      "email": "lasserafn@gmail.com"
    },
    {
      "name": "Rackbeat",
      "email": "open-source@rackbeat.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "lasserafn/php-initial-avatar-generator": "^4.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "laravel/pint": "^1.15",
    "phpunit/phpunit": "^7.0|^9.5.10|^10.1",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Rackbeat\\UIAvatars\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UIAvatars\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rackbeat\\UIAvatars\\UIAvatarsServiceProvider"
      ]
    }
  }
}
